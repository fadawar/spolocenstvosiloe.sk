<div class="row">
  <div class="col-sm-12">
    {% for tag in article.tags.all %}
      <a href="{% url "view_articles_with_tag" tag.id %}">
        <span class="label label-default">{{ tag.name }}</span>
      </a>
    {% endfor %}
    {% for tag in article.videoarticle.tags.all %}
      <a href="{% url "view_articles_with_tag" tag.id %}">
        <span class="label label-default">{{ tag.name }}</span>
      </a>
    {% endfor %}
  </div>
</div>